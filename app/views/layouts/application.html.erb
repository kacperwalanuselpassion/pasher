<!DOCTYPE html>
<html>
<head>
  <title>El Pasher</title>
  <%= favicon_link_tag 'favicon.png', type: 'image/png' %>
  <%= stylesheet_link_tag "application", media: "all" %>
  <%= stylesheet_link_tag 'http://fonts.googleapis.com/css?family=Open+Sans:400,600,700&subset=latin,latin-ext' %>
  <%= csrf_meta_tags %>
</head>
<body>

    <%= render 'shared/config' %>
    <% if flash[:notice] %>
        <div class="flash-notice-container">
            <div class="flash-notice">
              <%= flash[:notice] %>
            </div>
        </div>
    <% end %>
    <%= yield %>

<%= javascript_include_tag "application" %>

</body>
</html>
